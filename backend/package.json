{
  "name": "dmd-cottage-sheets-backend",
  "version": "1.0.0",
  "description": "Backend API для DMD Cottage Sheets",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "npx nodemon --exec 'npx ts-node --transpile-only' src/app.ts",
    "build": "tsc",
    "test": "jest",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "sequelize": "^6.35.2",
    "mysql2": "^3.6.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/joi": "^17.2.3",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "sequelize-cli": "^6.6.2"
  },
  "keywords": ["sheets", "collaboration", "api"],
  "author": "DMD Cottage",
  "license": "MIT"
} 